<?xml version='1.0' encoding='utf-8'?>
<widget id="org.esrgc.apps" version="1.1.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <name>UniteSalisbury</name>
    <description>
    Find events around town.
  </description>
    <author email="esrgc@salisbury.edu" href="http://www.esrgc.org">
    Eastern Shore Regional GIS Cooperative
  </author>
    <content src="index.html" />
    <access origin="http://apps.esrgc.org/uniteSalisbury" />
    <access origin="http://*.tiles.mapbox.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="data:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <icon height="180" src="assets/res/ios/AppIcon.appiconset/Icon-App-60x60@3x.png" width="180" />
    <icon height="60" src="assets/res/ios/AppIcon.appiconset/Icon-App-60x60@1x.png" width="60" />
    <icon height="120" src="assets/res/ios/AppIcon.appiconset/Icon-App-60x60@2x.png" width="120" />
    <icon height="76" src="assets/res/ios/AppIcon.appiconset/Icon-App-76x76@1x.png" width="76" />
    <icon height="152" src="assets/res/ios/AppIcon.appiconset/Icon-App-76x76@2x.png" width="152" />
    <icon height="228" src="assets/res/ios/AppIcon.appiconset/Icon-App-76x76@3x.png" width="228" />
    <icon height="40" src="assets/res/ios/AppIcon.appiconset/Icon-App-40x40@1x.png" width="40" />
    <icon height="80" src="assets/res/ios/AppIcon.appiconset/Icon-App-40x40@2x.png" width="80" />
    <icon height="50" src="assets/res/ios/AppIcon.appiconset/Icon-Small-50x50@1x.png" width="50" />
    <icon height="100" src="assets/res/ios/AppIcon.appiconset/Icon-Small-50x50@2x.png" width="100" />
    <icon height="72" src="assets/res/ios/AppIcon.appiconset/Icon-App-72x72@1x.png" width="72" />
    <icon height="144" src="assets/res/ios/AppIcon.appiconset/Icon-App-72x72@2x.png" width="144" />
    <icon height="167" src="assets/res/ios/AppIcon.appiconset/Icon-App-83.5x83.5@2x.png" width="167" />
    <icon height="20" src="assets/res/ios/AppIcon.appiconset/Icon-App-20x20@1x.png" width="20" />
    <icon height="29" src="assets/res/ios/AppIcon.appiconset/Icon-App-29x29@1x.png" width="29" />
    <icon height="58" src="assets/res/ios/AppIcon.appiconset/Icon-App-29x29@2x.png" width="58" />
    <icon height="87" src="assets/res/ios/AppIcon.appiconset/Icon-App-29x29@3x.png" width="87" />
    <icon height="57" src="assets/res/ios/AppIcon.appiconset/Icon-App-57x57@1x.png" width="57" />
    <icon height="114" src="assets/res/ios/AppIcon.appiconset/Icon-App-57x57@2x.png" width="114" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
</widget>

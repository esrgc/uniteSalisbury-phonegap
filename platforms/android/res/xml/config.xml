<?xml version='1.0' encoding='utf-8'?>
<widget id="org.esrgc.apps" version="1.1.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>UniteSalisbury</name>
    <description>
    Find events around town.
  </description>
    <author email="esrgc@salisbury.edu" href="http://www.esrgc.org">
    Eastern Shore Regional GIS Cooperative
  </author>
    <content src="index.html" />
    <access origin="http://apps.esrgc.org/uniteSalisbury" />
    <access origin="http://*.tiles.mapbox.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="data:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <icon density="ldpi" src="assets/res/android/ldpi/ic_launcher.png" />
    <icon density="mdpi" src="assets/res/android/mdpi/ic_launcher.png" />
    <icon density="hdpi" src="assets/res/android/hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="assets/res/android/xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="assets/res/android/xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="assets/res/android/xxxhdpi/ic_launcher.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
